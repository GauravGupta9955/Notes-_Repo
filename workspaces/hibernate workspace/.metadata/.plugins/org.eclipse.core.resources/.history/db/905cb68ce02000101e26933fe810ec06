package com.Telusko.Spring1;

public class Alien {
	
	private int age;
	private Laptop lap;
	
	public void code() {
		System.out.println("Coding");
		lap.compile();
	}

	public Laptop getLap() {
		return lap;
	}

	public void setLap(Laptop lap) {
		this.lap = lap;
	}

	public int getAge() {
		return age;
	}



	public Alien(int age, Laptop lap) {
		super();
		this.age = age;
		this.lap = lap;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	
}
