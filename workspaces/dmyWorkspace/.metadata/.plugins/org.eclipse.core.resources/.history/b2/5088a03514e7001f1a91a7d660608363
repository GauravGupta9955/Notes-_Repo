package codingProject;

import java.util.Arrays;
import java.util.List;

public class TofindoddorEvenfromArray {
public static void main(String[] args) {
	List<Integer> num=Arrays.asList(5,7,9,6,11,13,17,19,18,20,23);
	num.stream().filter(n->n%2==0).forEach(n->System.out.println("Even no."+n));
	
}
}
