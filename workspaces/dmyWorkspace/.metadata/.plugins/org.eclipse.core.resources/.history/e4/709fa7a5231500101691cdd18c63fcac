package StreamApi;

import java.util.stream.IntStream;

public class test1 {
	public static void main(String[] args) {
		int n[] = {1,3,4,5,6}
		int no=6;
		int sum = IntStream.rangeClosed(1, 6).sum();
		int real= IntStream.of(n).sum();
		
		int missing_number =sum-real;
		System.out.println(missing_number);
	}

}
