package StreamApi;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.HashMap;
import java.util.LinkedHashMap;
import java.util.LinkedHashSet;
import java.util.List;
import java.util.Map;
import java.util.Scanner;
import java.util.stream.Collectors;
import java.util.stream.IntStream;



public class test1 {
	public static void main(String[] args) {
		List<Integer> arr=List.of(2,3,5,-1,-5,-6,-9,1,4,-4,-11,12,13); 
		
		ArrayList<Character> positive = new ArrayList<>();
		ArrayList<Character> negative = new ArrayList<>();
		
		for(int i :arr) {
			Character ch=String.valueOf(i).charAt(0);
			if(Character.isDigit(ch))
				positive.add(ch);
			else
				negative.add(ch);
		}
		
		negative.forEach(s->System.out.print(s+" "));
		positive.forEach(s->System.out.print(s+" "));
		
		
	
	
	}
	
}
