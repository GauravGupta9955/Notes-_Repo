package StreamApi;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.HashMap;
import java.util.HashSet;
import java.util.LinkedHashSet;
import java.util.List;
import java.util.Map;
import java.util.Scanner;
import java.util.stream.Collectors;



//WAP to find the missing term from the array
public class test1 {
	public static void main(String[] args) {

		String[] str = { "nat", "bat", "ate", "eat", "tea", "tan" };

		Map<String, List<String>> map = new HashMap<>();

		for (String str1 : str) {
			String sorted = sorted(str1);
			if (!map.containsKey(sorted)) {
				map.put(sorted, new ArrayList<>());

				map.get(sorted).add(str1);
			}
		}
		
		List<List<String>> result =new ArrayList<>(map.values());
		System.out.println(result);
		
	}

	private static String sorted(String str) {
		// TODO Auto-generated method stub
		
		char[]str12=str.toCharArray();
		Arrays.sort(str12);
		
		return new String(str12);
	}


}
