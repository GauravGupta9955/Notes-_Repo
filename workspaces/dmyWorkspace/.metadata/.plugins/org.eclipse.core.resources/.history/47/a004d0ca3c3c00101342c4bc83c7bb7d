//WAP to find the missing term from the array

class Employe implements Comparable<Employe>{
	int age;
	String name;
	
	
	public Employe(int age, String name) {
		super();
		this.age = age;
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	@Override
	public int compareTo(Employee that) {
		// TODO Auto-generated method stub
		return this.age-that.age;
	}
	
	
	
}
public class test1 {
	public static void main(String[] args) {

		ArrayList<Employee> emp =new ArrayList<>();
		emp.add(new Employee(32,"Gaurav"));
		emp.add(new Employee(37,"Gautam"));
		emp.add(new Employee(22,"Saurav"));
		emp.add(new Employee(29,"Rahul"));
		
		Collections.sort(emp);
		
		for(Employee emp1:emp)
			System.out.println(emp1);
	}

	

}
