package javaString;

public class Mutable1 {
	public static void main(String[] args) {
		String s1 =new String("GauravKumarGupta");
		String s2 =new String(" ");
		
		int j=s1.length();
		for(int i=j;i>=0;i--) {
			int k=i-1;
			s2=s2+s1.charAt(i-1);
		}
		System.out.println(s2);
		
		
		  
	}
 
}
 