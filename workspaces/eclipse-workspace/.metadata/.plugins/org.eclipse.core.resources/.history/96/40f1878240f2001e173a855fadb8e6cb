
public class test123 {
public static void main(String[] args) {
	 int size =6-1;
	   for(int i=0;i<=size;i++){
	       for(int k=1;k<=(size-i);k++){
	            System.out.print(" ");
	       }
	       for(int j=0;j<=i;j++){
	           System.out.print("#");
	       }
	       System.out.println();
	   }

	    }
}
