package javaNewCode;

public class for_pattern4 {
	public static void main(String[] args) {

		int k = 5;
		for (int i = 0; i < k; i++) {
			for (int j = 0; j < k; j++) {
				if (i == k / 2 || (n == 0 && i == 1) || (n == 0 && i == 2) ||
						(n == 0 && i == 3)|| n == k - 1)
					System.out.print("*");
				else
					System.out.print(" ");
			}
			System.out.println();
		}

	}
}
