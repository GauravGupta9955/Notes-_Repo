import java.util.*;

class Result {

public static int birthdayCakeCandles(List<Integer> candles) {
	 int maximum =Collections.min(candles);
     int count=1;
     for(int candcout :candles){
         if(candcout==maximum)
         ++count;
         
     }
     return count;
}
}
public class test123 {
public static void main(String[] args) {
	List<Integer>arr = new ArrayList<Integer>(Arrays.asList(18,90,9013,90,75,90,8,90,43));
	int result = Result.birthdayCakeCandles(arr);
}
}
