package javaNewCode;

public class for_pattern4 {
	public static void main(String[] args) {

		int k = 5;
		for (int i = 0; i < k; i++) {
			for (int j = 0; j < k; j++) {
				if ((i == 0 && j != 0 && j != k - 1) || (j == 0 && i != 0) || i == k / 2)
					System.out.print("*");
				else if (i == 0 || i == k / 2 || j == 0 && j == k - 1)
					System.out.print("*");
				else
					System.out.println(" ");
			}
			System.out.println();
		}

	}
}
