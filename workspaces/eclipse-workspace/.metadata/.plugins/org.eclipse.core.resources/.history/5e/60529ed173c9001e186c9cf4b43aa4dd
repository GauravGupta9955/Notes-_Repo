package javaString;

public class Mutable1 {
	public static void main(String[] args) {
		String s1 =new String("gaurav kumar gupta");
		String s2 =new String();
	
		
		for(int i=0;i<=(s1.length()-1);i++) {
			if(s1.charAt(i)-32==0)
			s2=s2+(char)(s1.charAt(i)-32);
			else
			s2=s2+" ";
		}
		System.out.println(s2);
		
		
		  
	}
 
}
 