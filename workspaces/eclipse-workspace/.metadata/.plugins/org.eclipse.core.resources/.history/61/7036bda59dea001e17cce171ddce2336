
import java.util.*;

public class ArrayList2 {
	public static void main(String[] args) {
		List<Integer>list =new ArrayList<Integer>() ;
		list.add(50);
		list.add(20);
		list.add(10);
		list.add(5);
		list.add(4);
		list.add(3);
		
		Iterator it = list.iterator();
		while(it.hasNext()) {
			Integer i =(Integer) it.next();
			if(i>10)
				it.remove();
			
			else
				System.out.println(i);
		}
	}

}
